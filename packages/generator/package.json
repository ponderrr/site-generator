{
  "name": "@site-generator/generator",
  "version": "0.1.0",
  "description": "Site generation logic for the site generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "clean": "rm -rf dist .cache"
  },
  "dependencies": {
    "@site-generator/core": "workspace:*",
    "@site-generator/extractor": "workspace:*",
    "@site-generator/analyzer": "workspace:*",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "@shadcn/ui": "^0.0.4",
    "framer-motion": "^11.0.3",
    "sharp": "^0.33.2",
    "@vercel/og": "^0.6.2"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "typescript": "^5.4.0"
  }
}
